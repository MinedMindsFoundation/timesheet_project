<%=user_info[1]%> <%=user_info[2]%>:<br><br>
<table>
<tr>
<th>Employee ID</th>
<th>First Name</th>
<th>Last Name</th>
<th>Date Of Hire</th>
<th>Business Email</th>
<th>Admin?</th>
<th>PTO</th>
</tr>
<tr>
<%user_info.each do |item|%>
    <td><%=item%></td> 
<%end%>
</tr>
</table><br>
Pay Period: <%=pay_period[0]%> to <%=pay_period[1]%>
<table>
<th>Time in</th>
<th>Lunch Start</th>
<th>Lunch End</th>
<th>Time out</th>
<th>Date</th>
<%times.each do |item|%>
<tr>
<%item.each do |time|%>
<td><%if time.length == 5%>
    <%=time_converter(time)%>
    <%else%>
    <%=time%>
    <%end%>
</td>
<%end%>
</tr>
<%end%>
</table><br>
<form method = "post" action = "return">
<input type="submit" value="Return">
</form>