<form method="post" action="commits_to_send">
<div id="gitbox" class= "scrollable top_right" align = "right">
    <%git_commits.each do |repo|%>
        <h1><%=repo[0]%></h1>
        <%repo[1].each do |dates|%>
            <table align ="center">
            <h3><%=dates[0]%></h3>
                <th></th>
                <th>Branch</th>
                <th>SHA Key</th>
                <th>Commit</th>
                <%dates[1].each do |info|%>
                    <tr align ="center">
                    <%checked_info = Hash["repo"=>repo[0], "date"=>dates[0], "branch"=>info["branch"], "sha"=>info["sha"], "message"=>info["message"]]%>
                    <td><input type = "checkbox" name = "info[]" value = '<%=checked_info%>'></td>
                    <td><%=info["branch"]%></td>
                    <td><%=info["sha"]%></td>
                    <td><%=info["message"]%></td>
                    </tr>
                <%end%>
            </table>
        <%end%>
    <%end%>
</div>
<input type= "submit">
</form>