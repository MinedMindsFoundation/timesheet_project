<!-- Admins Employee Management -->
<!--sign out div-->
<center><div id="timeout">
</div>
<h1>Current List of Employees:</h1>
<table>
<tr>
<th>Select</th>
<th>Employee ID</th>
<th>First Name</th>
<th>Last Name</th>
</tr>
<%admin_list.each do |row|%>
<form method = "post" action = "update_emp">
    <tr>
    <td><input type = "checkbox" name = "info[]" value = "<%=row[0]%>"></td>
    <%row.each do |item|%>
        <td>
        <%=item.capitalize%>
        </td>
    <%end%>
    </tr>
<%end%>
</table><br>
    <input class="button" type="submit" name="choose" value="Info">
    <input class="button" type="submit" name="choose" value="Update">
    <input class="button" type="submit" name="choose" value="Delete"><br><br><br>
</form>
<form method = "post" action = "return">
    <input class="button" type="submit" value="Return">
</form>
<meta name="google-signin-client_id" content="529014474103-64pofb78h79be0rblagq6cil53svu4qb.apps.googleusercontent.com">
<!--sign out section--><br>
<!--toggle for sign out button-->
<%toggle = "social"%>
<%if toggle == "sso"%>
    <script src="/js/ssotimeout.js"></script>
    <body onload="ssoidleTimer()"></body>
    <%elsif toggle == "social"%>
    <!--social media section-->
    <button id = 'sign_out' onclick= 'logout()' hidden>Sign out</button>
    <!--facebook sign out-->
    <script src = "/js/facebook.js"></script>
    <!--google sign out-->
    <script src="https://apis.google.com/js/platform.js" async defer></script>
    <script src= "js/google_js.js"></script>
    <script src="https://apis.google.com/js/platform.js?onload=onLoad" async defer></script>
    <!--TIME OUT STUFF-->
    <script src="/js/timeout.js"></script>
    <body onload="idleTimer()"></body>
<%end%></center>